            {
                "session_id": "0604121d8907",
                "role": "Agent",
                "script_annotation": "为了完成冻结操作，请提供您的银行卡号和密码进行验证。",
                "action_annotation": "验证卡号密码"
            },
            {
                "session_id": "0604121d8907",
                "role": "User",
                "script_annotation": "我的银行卡号是123456****，密码是******。",
                "action_annotation": "密码正确"
            },
            {
                "session_id": "0604121d8907",
                "role": "Agent",
                "script_annotation": "账户已成功冻结，您的资金现在是安全的。我们会进一步调查这笔异常交易。",
                "action_annotation": "账户冻结"
            },
            {
                "session_id": "0604121d8907",
                "role": "User",
                "script_annotation": "谢谢你们的帮助。",
                "action_annotation": "感谢"
            },
            {
                "session_id": "0604121d8907",
                "role": "Agent",
                "script_annotation": "不客气，感谢您的配合，祝您生活愉快！",
                "action_annotation": "礼貌结束"
            },
            {
                "session_id": "0604121d8907",
                "role": "User",
                "script_annotation": "好的，拜拜！",
