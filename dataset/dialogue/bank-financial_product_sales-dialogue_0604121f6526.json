    "dialogue": {
        "dialogues": [
            {
                "session_id": "0604121f6526",
                "role": "Agent",
                "script_annotation": "您好，请问是袁*女士吗？我是理财产品销售客服。",
                "action_annotation": "打招呼"
            },
            {
                "session_id": "0604121f6526",
                "role": "User",
                "script_annotation": "您好，是的，我是袁*。",
                "action_annotation": "打招呼"
            },
            {
                "session_id": "0604121f6526",
                "role": "Agent",
                "script_annotation": "袁女士，我们目前有一款收益率高达5%的理财产品，投资期限为一年，风险评估为中低风险，非常适合年轻投资者。您感兴趣了解一下吗？",
                "action_annotation": "介绍理财产品"
            },
            {
                "session_id": "0604121f6526",
                "role": "User",
                "script_annotation": "听起来不错，可以详细介绍一下吗？",
                "action_annotation": "感兴趣"
            },
            {
                "session_id": "0604121f6526",
                "role": "Agent",
                "script_annotation": "好的，袁女士。在详细介绍之前，我需要先记录一下您的身份信息，可以吗？",
                "action_annotation": "记录用户身份信息"
            },
            {
                "session_id": "0604121f6526",
                "role": "User",
                "script_annotation": "可以的，我的身份证号是36052120010307****。",
                "action_annotation": "提供信息"
            },
            {
                "session_id": "0604121f6526",
                "role": "Agent",
                "script_annotation": "感谢您的配合，袁女士。这是付款链接，您可以通过这个链接完成支付。",
                "action_annotation": "发送付款链接"
            },
            {
                "session_id": "0604121f6526",
                "role": "User",
                "script_annotation": "好的，我已经收到了链接。",
                "action_annotation": "接受链接"
            },
            {
                "session_id": "0604121f6526",
                "role": "Agent",
                "script_annotation": "请您确认一下，您购买的是收益率为5%，投资期限为一年的理财产品，对吗？",
                "action_annotation": "确认购买信息"
            },
            {
                "session_id": "0604121f6526",
                "role": "User",
                "script_annotation": "是的，我确认购买。",
                "action_annotation": "确认购买"
            },
            {
                "session_id": "0604121f6526",
                "role": "Agent",
                "script_annotation": "感谢您的购买，袁女士。您的订单已经成功提交，我们会尽快为您办理。",
                "action_annotation": "购买成功"
            },
            {
                "session_id": "0604121f6526",
                "role": "User",
                "script_annotation": "谢谢您的帮助。",
                "action_annotation": "感谢"
            },
            {
                "session_id": "0604121f6526",
                "role": "Agent",
                "script_annotation": "不客气，袁女士。祝您投资顺利，生活愉快！",
                "action_annotation": "礼貌结束"
            },
            {
                "session_id": "0604121f6526",
                "role": "User",
                "script_annotation": "好的，再见！",
                "action_annotation": "结束"
            }
        ]
    },
