            {
                "session_id": "060412f2e49a",
                "role": "User",
                "script_annotation": "这个位置可以，那就选C排10号、11号吧。",
                "action_annotation": "确定座位号"
            },
            {
                "session_id": "060412f2e49a",
                "role": "Agent",
                "script_annotation": "好的，座位已为您锁定。接下来我将发起支付流程，请您按照指引完成支付操作。",
                "action_annotation": "发起支付"
            },
            {
                "session_id": "060412f2e49a",
                "role": "User",
                "script_annotation": "我已经支付成功了。",
                "action_annotation": "支付成功"
