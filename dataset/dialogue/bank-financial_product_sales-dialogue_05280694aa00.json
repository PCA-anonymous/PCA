    "dialogue": {
        "dialogues": [
            {
                "session_id": "05280694aa00",
                "role": "Agent",
                "script_annotation": "您好，请问是王秀*先生吗？",
                "action_annotation": "打招呼"
            },
            {
                "session_id": "05280694aa00",
                "role": "User",
                "script_annotation": "您好，是的，我是王秀*。",
                "action_annotation": "打招呼"
            },
            {
                "session_id": "05280694aa00",
                "role": "Agent",
                "script_annotation": "王先生，您好！我是理财产品销售客服。我们公司最近推出了一款收益率高、投资期限灵活的理财产品，适合像您这样的年轻投资者。您有兴趣了解一下吗？",
                "action_annotation": "介绍理财产品"
            },
            {
                "session_id": "05280694aa00",
                "role": "User",
                "script_annotation": "听起来不错，可以详细说说吗？",
                "action_annotation": "感兴趣"
            },
            {
                "session_id": "05280694aa00",
                "role": "Agent",
                "script_annotation": "好的，王先生。为了更好地为您服务，我需要先记录一下您的身份信息，可以吗？",
                "action_annotation": "记录用户身份信息"
            },
            {
                "session_id": "05280694aa00",
                "role": "User",
                "script_annotation": "可以的，我的身份证号是34060420020101****。",
                "action_annotation": "提供信息"
            },
            {
                "session_id": "05280694aa00",
                "role": "Agent",
                "script_annotation": "感谢您的配合。现在我会给您发送一个付款链接，您可以通过这个链接完成支付。",
                "action_annotation": "发送付款链接"
            },
            {
                "session_id": "05280694aa00",
                "role": "User",
                "script_annotation": "好的，我已经收到链接了。",
                "action_annotation": "接受链接"
            },
            {
                "session_id": "05280694aa00",
                "role": "Agent",
                "script_annotation": "请您确认一下购买信息，确认无误后点击支付即可。",
                "action_annotation": "确认购买信息"
            },
            {
                "session_id": "05280694aa00",
                "role": "User",
                "script_annotation": "我已经确认并完成支付了。",
                "action_annotation": "确认购买"
            },
            {
                "session_id": "05280694aa00",
                "role": "Agent",
                "script_annotation": "恭喜您，王先生，您的理财产品已经购买成功！",
                "action_annotation": "购买成功"
            },
            {
                "session_id": "05280694aa00",
                "role": "User",
                "script_annotation": "谢谢您的帮助。",
                "action_annotation": "感谢"
            },
            {
                "session_id": "05280694aa00",
                "role": "Agent",
                "script_annotation": "感谢您的配合，祝您生活愉快！",
                "action_annotation": "礼貌结束"
            },
            {
                "session_id": "05280694aa00",
                "role": "User",
                "script_annotation": "好的，拜拜！",
                "action_annotation": "结束"
            }
        ]
    },
