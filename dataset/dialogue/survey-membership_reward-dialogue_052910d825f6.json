            },
            {
                "session_id": "052910d825f6",
                "role": "User",
                "script_annotation": "好的，我收到了。",
                "action_annotation": "接收链接"
            },
            {
                "session_id": "052910d825f6",
                "role": "Agent",
                "script_annotation": "请您花几分钟时间填写问卷，填写完成后我们会为您开通7天腾讯视频会员。",
                "action_annotation": "等待填写"
