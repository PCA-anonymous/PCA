        "dialogues": [
            {
                "session_id": "98431ad1",
                "role": "Agent",
                "script_annotation": "您好，这里是银行客服中心，请问是刘*女士吗？",
                "action_annotation": "打招呼"
            },
            {
                "session_id": "98431ad1",
                "role": "User",
                "script_annotation": "您好，我是刘*。",
                "action_annotation": "本人"
            },
            {
                "session_id": "98431ad1",
                "role": "Agent",
                "script_annotation": "非常感谢您的配合。我们注意到您账户上有一笔异常的大额交易，请问这笔交易是您本人操作的吗？",
                "action_annotation": "询问大额交易记录"
            },
            {
                "session_id": "98431ad1",
                "role": "User",
                "script_annotation": "不，这笔交易不是我操作的。",
                "action_annotation": "否认交易"
            },
            {
                "session_id": "98431ad1",
                "role": "Agent",
                "script_annotation": "好的，为了保护您的资金安全，我们建议立即冻结您的账户。您同意吗？",
                "action_annotation": "紧急冻结询问"
            },
            {
                "session_id": "98431ad1",
                "role": "User",
                "script_annotation": "冻结账户会影响我的其他交易吗？",
                "action_annotation": "担忧疑惑"
            },
            {
                "session_id": "98431ad1",
                "role": "Agent",
                "script_annotation": "我理解您的担忧。冻结账户是为了防止进一步的损失，冻结后我们会尽快协助您处理这笔异常交易并恢复账户正常使用。",
                "action_annotation": "共情安抚"
            },
            {
                "session_id": "98431ad1",
                "role": "User",
                "script_annotation": "好的，那就请帮我冻结账户吧。",
                "action_annotation": "确认冻结"
            },
            {
                "session_id": "98431ad1",
                "role": "Agent",
                "script_annotation": "为了验证您的操作权限，请您提供银行卡号和账户密码。",
                "action_annotation": "验证卡号密码"
            },
            {
                "session_id": "98431ad1",
                "role": "User",
                "script_annotation": "银行卡号是123456****，密码是123456。",
                "action_annotation": "密码错误"
            },
            {
                "session_id": "98431ad1",
                "role": "Agent",
                "script_annotation": "非常抱歉，您提供的密码不正确。请您重新确认密码并再次联系我们。感谢您的配合，祝您生活愉快！",
                "action_annotation": "礼貌结束"
            },
            {
                "session_id": "98431ad1",
                "role": "User",
                "script_annotation": "好的，拜拜！",
                "action_annotation": "结束"
            }
        ]
    },
