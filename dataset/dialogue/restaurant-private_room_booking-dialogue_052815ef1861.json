        "dialogues": [
            {
                "session_id": "052815ef1861",
                "role": "Agent",
                "script_annotation": "您好，刘先生，这里是我们饭店的客服，请问有什么可以帮您的吗？",
                "action_annotation": "打招呼"
            },
            {
                "session_id": "052815ef1861",
                "role": "User",
                "script_annotation": "你好，我想预订一个包间。",
                "action_annotation": "提出预订意愿"
            },
            {
                "session_id": "052815ef1861",
                "role": "Agent",
                "script_annotation": "好的，请问您预订的包间大概有多少人参加？预订的时间是几点呢？",
                "action_annotation": "询问人数和时间"
            },
            {
                "session_id": "052815ef1861",
                "role": "User",
                "script_annotation": "这个我暂时还不确定。",
                "action_annotation": "拒绝提供人数或时间"
            },
            {
                "session_id": "052815ef1861",
                "role": "Agent",
                "script_annotation": "理解，刘先生。不过为了更好地为您安排合适的包间，方便您用餐体验，能否先提供一个大概的时间和人数呢？",
                "action_annotation": "尝试说服"
            },
            {
                "session_id": "052815ef1861",
                "role": "User",
                "script_annotation": "好吧，我明白了。",
                "action_annotation": "习惯性应答可以继续"
            },
            {
                "session_id": "052815ef1861",
                "role": "Agent",
                "script_annotation": "那么请问大概有多少人参加？预订的时间大概是几点呢？",
                "action_annotation": "询问人数和时间"
            },
            {
                "session_id": "052815ef1861",
                "role": "User",
                "script_annotation": "我还是不确定具体的人数和时间。",
                "action_annotation": "拒绝提供人数或时间"
            },
            {
                "session_id": "052815ef1861",
                "role": "Agent",
                "script_annotation": "好的，刘先生，没关系。如果您确定了具体的人数和时间，可以随时联系我们。祝您生活愉快！",
                "action_annotation": "礼貌结束"
            },
            {
                "session_id": "052815ef1861",
                "role": "User",
                "script_annotation": "好的，谢谢，再见！",
                "action_annotation": "结束"
            }
        ]
    },
