            },
            {
                "session_id": "06041384a74c",
                "role": "User",
                "script_annotation": "我选择线下取件。",
                "action_annotation": "线下取件"
            },
            {
                "session_id": "06041384a74c",
                "role": "Agent",
                "script_annotation": "明白了。我们的快递站工作时间是每天早上8点到下午5点，请问您方便什么时候来取件呢？",
                "action_annotation": "选择约定时间"
            },
            {
                "session_id": "06041384a74c",
                "role": "User",
                "script_annotation": "我会在工作时间内过去取件。",
                "action_annotation": "选择工作时间"
            },
            {
                "session_id": "06041384a74c",
                "role": "Agent",
                "script_annotation": "好的，高先生，我们已经记录下您的取件安排，您可以在工作时间内来我们的快递站取件。感谢您的配合。",
                "action_annotation": "约定成功"
            },
            {
                "session_id": "06041384a74c",
                "role": "User",
                "script_annotation": "谢谢您的帮助。",
                "action_annotation": "感谢"
            },
            {
                "session_id": "06041384a74c",
                "role": "Agent",
                "script_annotation": "感谢您的配合，祝您生活愉快！",
                "action_annotation": "礼貌结束"
            },
            {
                "session_id": "06041384a74c",
                "role": "User",
                "script_annotation": "好的，拜拜！",
                "action_annotation": "结束"
            }
