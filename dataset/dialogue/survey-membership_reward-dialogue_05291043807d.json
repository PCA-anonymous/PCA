    "dialogue": {
        "dialogues": [
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "您好，马先生！我是腾讯视频客服，很高兴为您服务。",
                "action_annotation": "打招呼"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "您好，有什么事吗？",
                "action_annotation": "打招呼"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "请问您是马*先生吗？",
                "action_annotation": "核对用户身份"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "是的，我是马*。",
                "action_annotation": "本人"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "马先生，我们正在进行一项用户满意度调查。参与问卷调查的用户将获得7天的腾讯视频会员，您是否愿意参加呢？",
                "action_annotation": "宣传填问卷送会员活动"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "好的，我愿意参加。",
                "action_annotation": "愿意参加"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "非常感谢！我现在通过短信发送问卷链接给您，请您查收。",
                "action_annotation": "发送问卷链接"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "好的，我收到了。",
                "action_annotation": "接收链接"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "感谢您的配合，请您抽出几分钟时间填写问卷。",
                "action_annotation": "等待填写"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "我现在有点忙，稍后再填写可以吗？",
                "action_annotation": "未填写"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "理解您的忙碌，问卷只需几分钟时间，填写后您就能立即获得7天会员奖励哦。",
                "action_annotation": "尝试说服"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "那好吧，我稍后再看看。",
                "action_annotation": "拖延决策"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "我理解您的时间宝贵，您可以随时填写问卷，我们的会员奖励会为您保留。感谢您的支持！",
                "action_annotation": "共情安抚"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "好了，我已经填写完了。",
                "action_annotation": "填写完成"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "感谢您的配合，请您提供一下QQ号，我们将为您开通7天的腾讯视频会员。",
                "action_annotation": "询问QQ号"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "我的QQ号是12345678。",
                "action_annotation": "提供QQ号"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "好的，我们已经为您开通了7天的腾讯视频会员，请您查收。",
                "action_annotation": "开通会员"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "我看到了，已经开通成功了，谢谢。",
                "action_annotation": "成功开通"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "不客气，感谢您的参与和支持，祝您观影愉快！",
                "action_annotation": "感谢"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "谢谢你们的服务。",
                "action_annotation": "感谢"
            },
            {
                "session_id": "05291043807d",
                "role": "Agent",
                "script_annotation": "感谢您的配合，祝您生活愉快！",
                "action_annotation": "礼貌结束"
            },
            {
                "session_id": "05291043807d",
                "role": "User",
                "script_annotation": "好的，拜拜！",
                "action_annotation": "结束"
            }
        ]
    },
